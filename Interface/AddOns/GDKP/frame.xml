							local update = self:GetProgress()
							local cury = self.startpointy - update*(self.startpointy-self.endpointy)
							local point,ui,refPoint,x = self.expandButton:GetPoint(1)
							self.expandButton:ClearAllPoints()
							self.expandButton:SetPoint(point,ui,refPoint,x,cury)
						</OnUpdate>
					</Scripts>
				</Animation>-->
				<Animation  duration="0.2" maxFramerate="180">
					<Scripts>
						<OnLoad>
							self.regionParent = self:GetRegionParent()
							self.maxy = 190
							self.miny = 115
						</OnLoad>
						<OnPlay>
							self.regionParent:SetMovable(false)
							_G[self.regionParent:GetName().."ExpandButton"]:Disable()
						</OnPlay>
						<OnUpdate>
							local update = self:GetProgress()
							local cury = update*(self.maxy-self.miny)+ self.miny
							self.regionParent:SetHeight(cury);
						</OnUpdate>
						<OnFinished>
							GDKP_MainFrameShowButtons()
							self:GetRegionParent():SetMovable(true)
							_G[self.regionParent:GetName().."ExpandButton"]:Enable()
							_G[self.regionParent:GetName().."ExpandButtonExpandTexture"]:SetRotation(math.pi)

						</OnFinished>
					</Scripts>
				</Animation>
			</AnimationGroup>
			<AnimationGroup name="$parentAnimGroupCol" looping="NONE" smoothing ="IN_OUT">
				<Animation  duration="0.2" maxFramerate="180">
					<Scripts>
						<OnLoad>
							self.regionParent = self:GetRegionParent()
							self.maxy = 190
							self.miny = 115
						</OnLoad>
						<OnPlay>
							GDKP_MainFrameHideButtons()
							self.regionParent:SetMovable(false)
							_G[self.regionParent:GetName().."ExpandButton"]:Disable()
						</OnPlay>
						<OnUpdate>
							local update = self:GetProgress()
							local cury = self.maxy - (self.maxy - self.miny)*update
							self.regionParent:SetHeight(cury);
						</OnUpdate>
						<OnFinished>
							self:GetRegionParent():SetMovable(true)
							_G[self.regionParent:GetName().."ExpandButton"]:Enable()
							_G[self.regionParent:GetName().."ExpandButtonExpandTexture"]:SetRotation(0)
							self.regionParent:SetHeight(115);
						</OnFinished>
					</Scripts>
				</Animation>

			</AnimationGroup>
		</Animations>

