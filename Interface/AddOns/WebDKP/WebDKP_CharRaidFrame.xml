                        WebDKP_UpdateCharRaidLogTable(self);
                    </OnClick>
                </Scripts>
            </CheckButton>
            <!-- Filter the data based on how far to go back -->
            <EditBox name="$parentFilterDate" inherits="WebDKPEditBoxTemplate">
                <Size>
                    <AbsDimension x="75" y="32"></AbsDimension>
                </Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentRemChar">
                        <Offset>
                            <AbsDimension x="95" y="-25"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" inherits="GameFontNormal">
                            <Anchors>
                                <Anchor point="TOPLEFT">
                                    <Offset>
                                        <AbsDimension x="-90" y="-8"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad>
                        getglobal(self:GetParent(self):GetName(self).."FilterDate"):SetAutoFocus( false );
                    </OnLoad>
                    <OnEscapePressed>
                        getglobal(self:GetParent(self):GetName(self).."FilterDate"):ClearFocus(self);
                    </OnEscapePressed>
                    <OnEnterPressed>
                        <!-- Does nothing when you press enter currently -->
                    </OnEnterPressed>
                    <OnTextChanged>
                        <!-- Does nothing when you change the text. It will only do something when you hit the button -->
                    </OnTextChanged>
                </Scripts>
            </EditBox>
            <!-- Apply the filter -->
            <CheckButton name="$parentApplyFilterDate" inherits="OptionsCheckButtonTemplate" checked="false">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$parentFilterDate">
