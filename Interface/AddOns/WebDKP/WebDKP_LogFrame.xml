<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
..\..\FrameXML\UI.xsd">
    <!-- Define a template for a log entry -->
    <Frame name="WebDKP_LogLineTemplate" virtual="true">
        <Size>
            <AbsDimension x="520" y="20"/>
        </Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentBackground">
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                    <Color r="1" g="1" b="1" a="1"/>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <FontString name="$parentAwarded" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="5" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentAmount" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="115" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentReason" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="160" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentDate" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="350" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button name="$parentDel" hidden="false">
                <Size>
                    <AbsDimension x="20" y="20"/>
                </Size>
                <Anchors>
                    <Anchor point="LEFT">
                        <Offset>
                            <AbsDimension x="500" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        WebDKP_LogLineDel_OnClick(self);
                    </OnClick>
                    <OnLoad>
                        self:GetNormalTexture():SetTexCoord(0.2, 0.8, 0.2, 0.8);
                        self:GetPushedTexture():SetTexCoord(0.2, 0.8, 0.2, 0.8);
                    </OnLoad>
                </Scripts>
                <NormalTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Up"/>
                <PushedTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Down"/>
                <HighlightTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Highlight" alphaMode="ADD"/>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad>
                getglobal(self:GetName(self) .. "Background"):SetVertexColor(0, 0, 0, 0);
            </OnLoad>
            <OnEnter>
                WebDKP_HandleMouseOverLog(self);
            </OnEnter>
            <OnLeave>
                WebDKP_HandleMouseLeaveLog(self);
            </OnLeave>
            <OnMouseDown>
                WebDKP_SelectLogToggle(self);
            </OnMouseDown>
        </Scripts>
    </Frame>

