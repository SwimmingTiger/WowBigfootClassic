                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="5" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad>
                getglobal(self:GetName(self) .. "Background"):SetVertexColor(0, 0, 0, 0);
            </OnLoad>
            <OnEnter>
                <!-- WebDKP_HandleMouseOverLog(self); -->
            </OnEnter>
            <OnLeave>
                <!-- WebDKP_HandleMouseLeaveLog(self); -->
            </OnLeave>
            <OnMouseDown>
                WebDKP_SelectAwardeeLogToggle(self)
