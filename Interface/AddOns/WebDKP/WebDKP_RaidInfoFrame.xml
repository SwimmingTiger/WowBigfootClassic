    <!-- Define a template for a log entry -->
    <Frame name="WebDKP_RaidLogLineTemplate" virtual="true">
        <Size>
            <AbsDimension x="515" y="20"/>
        </Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentBackground">
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                    <Color r="1" g="1" b="1" a="1"/>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <FontString name="$parentRaidLocation" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="5" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentdate" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="130" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentstartTime" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="220" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentendTime" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="320" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parenttotaltime" inherits="ChatFontNormal" text="">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="420" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad>
                getglobal(self:GetName(self) .. "Background"):SetVertexColor(0, 0, 0, 0);
            </OnLoad>
            <OnEnter>
                WebDKP_HandleMouseOverRaidLog(self);
            </OnEnter>
            <OnLeave>
                WebDKP_HandleMouseLeaveRaidLog(self);
            </OnLeave>
            <OnMouseDown>
                WebDKP_SelectRaidLogToggle(self);
            </OnMouseDown>
        </Scripts>
    </Frame>

