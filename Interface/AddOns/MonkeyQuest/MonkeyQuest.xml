				self:SetBackdrop({
					bgFile = "Interface\\AddOns\\MonkeyLibrary\\Textures\\BackDrop.tga", 
					edgeFile = "Interface\\Tooltips\\UI-Tooltip-Border",
					tile = true,
					tileSize = 16,
					edgeSize = 16,
					insets = {left = 5, right = 5, top = 5, bottom = 5},
				})
				MonkeyQuest_OnLoad(self);
			</OnLoad>
