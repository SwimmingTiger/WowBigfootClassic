                    </OnLeave>
                    <OnMouseDown>
                        if self:IsEnabled() then
                            self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 1, -1);
                        end
                    </OnMouseDown>
                    <OnMouseUp>
                        self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 0, 0);
                    </OnMouseUp>
                    <OnClick>
                        local parent = self:GetParent()
                        parent.scrollFrame:Fire('OnItemDel', parent, parent.item)
