                    <TexCoords left="0" right="0.15625" top="0" bottom="1.0"/>
                </Texture>
                <Texture parentKey="Middle" file="Interface\PaperDollInfoFrame\UI-Character-InActiveTab">
                    <Size x="88" y="32"/>
